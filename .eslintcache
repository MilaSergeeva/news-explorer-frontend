);\n    }\n\n    return res.json();\n  });\n\nexport const authorize = (email, password) =>\n  fetch(`${BASE_URL}/signin`, {\n    method: 'POST',\n    headers: {\n      Accept: 'application/json',\n      'Content-Type': 'application/json',\n    },\n    body: JSON.stringify({ email, password }),\n  }).then((res) => {\n    if (!res.ok) {\n      return Promise.reject(`Ошибка: ${res.status}`);\n    }\n\n    return res.json();\n  });\n\nexport const getContent = (token) =>\n  fetch(`${BASE_URL}`, {\n    method: 'GET',\n    headers: {\n      Accept: 'application/json',\n      'Content-Type': 'application/json',\n      Authorization: `Bearer ${token}`,\n    },\n  }).then((res) => {\n    if (!res.ok) {\n      return Promise.reject(`Ошибка: ${res.status}`);\n    }\n\n    return res.json();\n  });\n","/Users/mila/dev/diplom/news-explorer-frontend/src/utils/token.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/Login/Login.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/Register/Register.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/PopupWithForm/PopupWithForm.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/InfoTooltip/InfoTooltip.js",["142","143"],"/Users/mila/dev/diplom/news-explorer-frontend/src/hooks/useForm.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/utils/Api.js",["144","145","146","147","148"],"import qs from 'querystring';\n\nclass Api {\n  constructor({ baseUrl, headers }) {\n    // options\n    this.baseUrl = baseUrl;\n    this.headers = headers;\n  }\n\n  makeRequest(endpoint, httpMethod, payload) {\n    let url = `${this.baseUrl}/${endpoint}`;\n\n    const options = {\n      method: `${httpMethod}`,\n      headers: this.headers,\n    };\n\n    if (\n      payload !== undefined &&\n      ['POST', 'PUT', 'PATCH'].includes(httpMethod)\n    ) {\n      options.body = JSON.stringify(payload);\n    } else if (payload !== undefined) {\n      url = `${url}?${qs.encode(payload)}`;\n    }\n\n    // TODO add catches\n    return fetch(url, options).then((res) => {\n      if (res.ok) {\n        // status code check\n        return res.json(); // process response\n      }\n\n      // server respond with error 4xx\n      if (res.status >= 400 && res.status < 500) {\n        return res\n          .json()\n          .then((body) =>\n            Promise.reject(`Что-то пошло не так: ${body.message}`),\n          );\n      }\n\n      // server didn't reply 5xx\n      return Promise.reject(`Что-то пошло не так: ${res.status}`);\n    });\n  }\n\n  getSavedNews() {\n    const endpoint = 'articles';\n    const httpMethod = 'GET';\n\n    return this.makeRequest(endpoint, httpMethod);\n  }\n\n  saveNews(newsPayload) {\n    const endpoint = 'articles';\n    const httpMethod = 'POST';\n\n    return this.makeRequest(endpoint, httpMethod, newsPayload);\n  }\n\n  deleteSavedNews(newsId) {\n    const endpoint = `articles/${newsId}`;\n    const httpMethod = 'DELETE';\n\n    return this.makeRequest(endpoint, httpMethod);\n  }\n\n  getUserInfo() {\n    const endpoint = 'users/me';\n    const httpMethod = 'GET';\n\n    return this.makeRequest(endpoint, httpMethod);\n  }\n\n  getNews(apiKey, query, fromDate, toDate, pageSize) {\n    const endpoint = 'everything';\n    const httpMethod = 'GET';\n    const payload = {\n      apiKey,\n      q: query,\n      from: fromDate,\n      to: toDate,\n      pageSize,\n    };\n\n    return this.makeRequest(endpoint, httpMethod, payload);\n  }\n}\n\nexport default Api;\n","/Users/mila/dev/diplom/news-explorer-frontend/src/components/Navigation/Navigation.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/About/About.js",["149"],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/NewsCardList/NewsCardList.js",["150","151"],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/SavedNewsHeader/SavedNewsHeader.js",[],"/Users/mila/dev/diplom/news-explorer-frontend/src/components/UserArticlesInfo/Use